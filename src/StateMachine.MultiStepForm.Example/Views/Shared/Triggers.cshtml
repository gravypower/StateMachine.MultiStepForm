<ul>
    @foreach (TriggerButton t in ViewBag.Triggers)
    {
        <li>
            <button name="triggerToken" type="submit" value="@t.TriggerToken">
                @t.Trigger.Description
            </button>
        </li>
    }
</ul>
<input type="hidden" name="stateToken" value="@ViewBag.State"/>